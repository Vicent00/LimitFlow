{
  "name": "limitflow-backend",
  "version": "1.0.0",
  "description": "Backend for LimitFlow DEX",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server/index.ts",
    "build": "tsc",
    "start": "node dist/server/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:register": "node --experimental-vm-modules node_modules/jest/bin/jest.js src/tests/auth/register.test.ts --coverage",
    "test:login": "node --experimental-vm-modules node_modules/jest/bin/jest.js src/tests/auth/login.test.ts --coverage",
    "test:server": "node --experimental-vm-modules node_modules/jest/bin/jest.js src/tests/server.test.ts --coverage",
    "test:signature": "node --experimental-vm-modules node_modules/jest/bin/jest.js src/tests/auth/signature.test.ts --coverage",
    "test:blockchain": "node --experimental-vm-modules node_modules/jest/bin/jest.js src/tests/blockchain.test.ts --coverage",
    "test:full-test-login": "node --experimental-vm-modules node_modules/jest/bin/jest.js src/tests/full-test-login.test.ts --coverage",
    "test:blockchain-integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js src/tests/blockchain-integration.test.ts --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:init": "ts-node scripts/init-db.ts",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "blockchain",
    "trading",
    "limit-orders"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@trpc/client": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "@types/axios": "^0.9.36",
    "@types/ioredis": "^4.28.10",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.18.3",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "next-auth": "^4.24.11",
    "superjson": "^2.2.1",
    "viem": "^2.30.1",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3"
  }
}
